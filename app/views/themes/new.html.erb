<%= breadcrumb(links: [
                {key: 'themes', name: t('themes.themes'), url: themes_path}, 
                {key: '', name: t('themes.add_new_theme')}
                ]) %>

<div class="panel">
    <div class="panel-body">
      <div class="col-md-10 col-md-offset-2 m-t m-b">
      <%= form_for(@theme, validate: true, html: {class: "form-horizontal"}) do |f| %>
        <%= render partial: 'form', locals: {f: f} %>
      <% end %>
    </div>
  </div>
</div>